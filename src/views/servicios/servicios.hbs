    };
   
    if ((isNaN(trackNumber) || trackNumber == "") && (trackType.selectedIndex == 0 || trackType.selectedIndex == 3)) {
      alert((trackType.selectedIndex == 0) ? "El número de guía debe ser numérico" : "El código de solicitud de recolección debe ser numérico");
      return false;
    }
    if ((trackNumber == "" || trackNumber == "0") && trackType.selectedIndex == 0) {
      alert("Ingresa el número de guía");
      return false;
    } else if ((trackNumber == "" || trackNumber == "0") && trackType.selectedIndex == 1) {
      alert("Ingresa el número de remisión");
      return false;
    } else if ((trackNumber == "" || trackNumber == "0") && trackType.selectedIndex == 2) {
      alert("Ingresa el número de factura");
